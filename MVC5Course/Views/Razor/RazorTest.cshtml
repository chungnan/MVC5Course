@{
    var items = new int[] { 1, 2, 3, 4, 5 };
}

@for (int i = 0; i < items.Length; i++)
{
    @items[i]
}


@for (int i = 0; i < items.Length; i++)
{
    @(items[i] + " ")
}


<ul>
@for (int i = 0; i < items.Length; i++)
{
  <li>@items[i]</li>
}
</ul>


